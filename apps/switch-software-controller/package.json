{
  "name": "@switch-software-controller/switch-software-controller",
  "productName": "Switch Software Controller",
  "version": "0.0.4",
  "description": "Software controller for Nintendo Switch",
  "author": "carimatics",
  "license": "MIT",
  "keywords": [],
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "sign": "codesign --force --deep --sign - out/Switch\\ Software\\ Controller-darwin-arm64/Switch\\ Software\\ Controller.app",
    "package": "electron-forge package",
    "package:mac": "electron-forge package && npm run sign",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "biome format --write ./src",
    "lint": "biome lint --write ./src",
    "check": "biome check --write ./src",
    "postinstall": "../../node_modules/.bin/electron-rebuild"
  },
  "dependencies": {
    "@electron/remote": "2.1.3",
    "@switch-software-controller/controller": "^0.0.4",
    "electron-squirrel-startup": "1.0.1",
    "react-icons": "5.5.0",
    "serialport": "^13.0.0",
    "vite-plugin-electron-renderer": "0.14.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.8.1",
    "@electron-forge/maker-deb": "7.8.1",
    "@electron-forge/maker-rpm": "7.8.1",
    "@electron-forge/maker-squirrel": "7.8.1",
    "@electron-forge/maker-zip": "7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "7.8.1",
    "@electron-forge/plugin-fuses": "7.8.1",
    "@electron-forge/plugin-vite": "7.8.1",
    "@electron/fuses": "1.8.0",
    "@electron/rebuild": "4.0.1",
    "@switch-software-controller/controller-api": "^0.0.4",
    "@switch-software-controller/serial-port-api": "^0.0.4",
    "@types/electron-squirrel-startup": "1.0.2",
    "electron": "37.2.1"
  },
  "gitHead": "5c9c65a448b90bc6249ef08f6806220359b506e6"
}
